<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.game.dao.GameNpcDao">

    <select id="getOne" parameterType="java.lang.String" resultType="com.game.entity.GameNpc">
        SELECT
        code AS code,
        `name` AS `name`,
        `desc` AS `desc`,
        level_num AS levelNum
        FROM game_npc
        WHERE
        code = #{code}
    </select>

</mapper>